    "postversion": "npm publish",
    "prepublishOnly": "git push origin --follow-tags"
  },
  "devDependencies": {
    "@types/node": "^17.0.42",
    "@types/tap": "^15.0.7",
    "clock-mock": "^1.0.6",
    "prettier": "^2.7.0",
    "tap": "^16.0.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=12"
  },
  "tap": {
    "nyc-arg": [
      "--include=index.js"
    ],
    "node-arg": [
      "--require",
      "ts-node/register"
